package arraysDemo;

public class Main {
	public static void main(String[] args) {
		
		String[] ogrenciler = new String[4];
		ogrenciler[0] = "Engin";
		ogrenciler[1] = "sdfdsf";
		ogrenciler[2] = "Essdngin";
		ogrenciler[3] = "sdfsd";
		
		
		
		for(String ogrenci:ogrenciler) {
			System.out.println(ogrenci);
		}
		
	}

}
