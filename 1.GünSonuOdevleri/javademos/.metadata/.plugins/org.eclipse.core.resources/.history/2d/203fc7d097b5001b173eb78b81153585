package loopDemo;       

public class Main {
	
	public static void main(String[] args) {
		
		for(int i=1;i<10;i++) {
			System.out.println(i);
		}
		System.out.println(" for döngüsü bitti");
		
		//while
		
		
		int i = 1;       
		while(i<10) {
			System.out.println(i);
			i+=2;
		}
		
		System.out.println(" while döngüsü bitti");
		
		
		// Do-While 
		int j=1;
		do {                                 // önce do çalýþýr. sonrasýnda koþulu saðlarsa while.
			System.out.println(j);
			j+=2;
		}while(j<10);
		
		
	}

}
